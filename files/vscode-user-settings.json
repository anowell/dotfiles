// Place your settings in this file to overwrite the default settings
{
  "search.exclude": {
    "**/target": true,
    "**/tmp": true
  },
  "files.trimTrailingWhitespace": true,
  "window.zoomLevel": 0,
  "editor.wordWrap": true,
  "workbench.colorTheme": "Visual Studio Dark",
  "rust.rls": {
    "executable": "rustup",
    "args": ["run", "nightly", "rls"]
  }
}
